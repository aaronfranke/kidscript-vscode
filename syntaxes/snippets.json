			"loop 5",
			"  var x = 20",
			"  loop 5",
			"    var item = new ${1:Class}(x, y)",
			"    $2",
			"    x = x + 15",
			"  end",
			"  y = y + 10",
			"end"
		],
		"description": "Creates a 5x5 grid of objects.\n"
	},
