{
	"name": "kidscript-vscode",
	"displayName": "KidScript",
	"description": "KidScript syntax highlighting support for VS Code. KidScript is a programming language created by Codeverse.",
	"homepage": "https://www.codeverse.com/",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aaronfranke/kidscript-vscode"
	},
	"publisher": "aaronfranke",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "kidscript",
				"aliases": [
					"KidScript",
					"kidscript"
				],
				"extensions": [
					".ks",
					".kidscript"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "kidscript",
				"scopeName": "source.kidscript",
				"path": "./syntaxes/kidscript.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "kidscript",
				"path": "./syntaxes/snippets.json"
			}
		],
		"iconThemesButDisabled": [
			{
				"id": "kidscript",
				"label": "KidScript",
				"path": "./theme/kidscript-theme.json"
			}
		]
	},
	"__metadata": {
		"id": "1451eb83-9f22-45fc-8b6a-069fe50346c3",
		"publisherDisplayName": "Aaron Franke",
		"publisherId": "65f2ccf5-fde8-4245-8633-a58ed3cc11b8"
	}
}
