{
	"name": "kidscript-vscode",
	"displayName": "KidScript",
	"description": "KidScript syntax highlighting support for VS Code. KidScript is a programming language created by Codeverse, which draws inspiration from, and acts as a gateway to, many popular languages.",
	"homepage": "https://www.codeverse.com/",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aaronfranke/kidscript-vscode"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "kidscript",
			"aliases": ["KidScript", "kidscript"],
			"extensions": [".ks",".kidscript"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "kidscript",
			"scopeName": "source.kidscript",
			"path": "./syntaxes/kidscript.tmLanguage.json"
		}],
		"snippets": [{
			"language": "kidscript",
			"path": "./syntaxes/snippets.json"
		}],
		"iconThemesButDisabled": [{
			"id": "kidscript",
			"label": "KidScript",
			"path": "./theme/kidscript-theme.json"
		}]
	}
}
